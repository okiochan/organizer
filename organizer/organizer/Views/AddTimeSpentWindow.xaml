<Window x:Class="organizer.Views.AddTimeSpentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:organizer.Views"
        mc:Ignorable="d"
        Title="AddTimeSpentWindow" Height="300" Width="300">

    <Grid>
        <Button x:Name="button" Content="Ok" HorizontalAlignment="Left" Margin="108,197,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="hoursBox" HorizontalAlignment="Left" Height="23" Margin="77,126,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="39"/>
        <TextBox x:Name="minutesBox" HorizontalAlignment="Left" Height="23" Margin="174,126,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="42"/>
        <Label x:Name="label" Content="Hours" HorizontalAlignment="Left" Margin="77,98,0,0" VerticalAlignment="Top" Height="23"/>
        <Label x:Name="label_Copy" Content="Minutes" HorizontalAlignment="Left" Margin="168,98,0,0" VerticalAlignment="Top" Height="23"/>

    </Grid>
</Window>

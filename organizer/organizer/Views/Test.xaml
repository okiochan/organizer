<Window x:Class="organizer.Views.Test"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:organizer.Views"
        mc:Ignorable="d" Loaded="Window_Loaded"
        Title="Test" Height="500" Width="500">
    <Grid>

        <StackPanel Name="panel">

        </StackPanel>

        <Canvas Name="canv" Width="500" Height="500" >
            <Ellipse Fill="Gainsboro" Canvas.Top="50" Width="350" Height="350" Canvas.Left="80" />
            
            <Rectangle Fill="LightBlue" Canvas.Left="240" Canvas.Top="210" Width="30" Height="30" />
            <Rectangle Fill="LightPink" Canvas.Left="240" Canvas.Top="50" Width="30" Height="30" />
            
        </Canvas>

    </Grid>
</Window>
